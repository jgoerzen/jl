keyer.port=COM4
keyer.baudrate=9600
keyer.timeout=15
keyer.numclips=8
keyer.ph.audioDir=audioclips
keyer.cw.minspeed=10
keyer.cw.maxspeed=40
keyer.cw.dashDotRatio=3.0
keyer.cw.markSpaceRatio=1.0
keyer.cw.charSpaceRatio=2.7
keyer.cw.wordSpaceRatio=8.0
keyer.cw.truncateZerosInSerials=true
keyer.cw.useCutZerosInQTCs=true
keyer.cw.initspeed=30
keyer.cw.showCWOnConsole=false

keyer.label.0=cq
keyer.ph.clip.0=cq-HACK.wav
keyer.cw.clip.0=cq na $f $f na
keyer.loopdelay.0=5

keyer.label.1=CALL
keyer.cw.clip.1=$f
keyer.loopdelay.1=0

keyer.label.2=Hunt
keyer.loopdelay.2=0
keyer.cw.clip.2=$t $f tu $n mike ct

keyer.label.3=Run
keyer.cw.clip.3=$t tu $n mike ct $f
keyer.loopdelay.3=0

keyer.label.4=name
keyer.loopdelay.4=0
keyer.cw.clip.4=mike

keyer.label.5=number
keyer.loopdelay.5=0
keyer.cw.clip.5=$n

keyer.label.6=state
keyer.cw.clip.6=ct
keyer.loopdelay.6=0

keyer.label.7=call
keyer.cw.clip.7=w1jq
keyer.loopdelay.7=0